            xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.newViewBagheri.CommentingPanel">
    <children>
        <TextField fx:id="title" layoutX="145.0" layoutY="67.0" prefHeight="25.0" prefWidth="310.0" promptText="Title"/>
        <TextArea fx:id="content" layoutX="145.0" layoutY="103.0" prefHeight="200.0" prefWidth="310.0"
                  promptText="Content"/>
        <Button layoutX="399.0" layoutY="312.0" mnemonicParsing="false" onAction="#addComment" text="submit"/>
        <Button layoutX="336.0" layoutY="312.0" mnemonicParsing="false" onAction="#goBack" text="cancel"/>
    </children>
</AnchorPane>
