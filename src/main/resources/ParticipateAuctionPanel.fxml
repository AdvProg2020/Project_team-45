<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import client.newViewHatami.ValidatorField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.newViewBagheri.ParticipateAuctionPanel">
    <ValidatorField fx:id="proposedPrice" validation="naturalNumber" layoutX="25.0" layoutY="35.0" prefHeight="25.0"
                    prefWidth="200.0" promptText="Proposed Price"/>
    <Label fx:id="massageLabel" layoutX="25.0" layoutY="71.0" prefWidth="200.0" textFill="red"/>
    <Button layoutX="172.0" layoutY="97.0" mnemonicParsing="false" onAction="#submitProposedPrice" text="submit"/>
    <Button layoutX="114.0" layoutY="97.0" mnemonicParsing="false" onAction="#goBack" text="cancel"/>
</AnchorPane>